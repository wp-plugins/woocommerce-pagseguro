msgid "Gateway Disabled"
msgstr "Gateway desabilitado"

#: wc-pagseguro.php:123
#@ wcpagseguro
msgid "PagSeguro does not support your store currency."
msgstr "PagSeguro não suporta a moeda atual utilizada na sua loja."

